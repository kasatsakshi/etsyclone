{
  "name": "etsyback",
  "version": "1.0.0",
  "description": "Etsy Node backend server",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run knex-migrate && nodemon --experimental-specifier-resolution=node server.js",
    "start": "npm run knex-migrate && node --experimental-specifier-resolution=node server.js",
    "knex": "node node_modules/.bin/knex",
    "knex-migrate": "npm run knex migrate:latest",
    "knex-rollback": "npm run knex migrate:down",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "Sakshi Kasat",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-mysql-session": "^2.1.7",
    "express-session": "^1.17.2",
    "formidable": "^2.0.1",
    "knex": "^1.0.3",
    "multer": "^1.4.4",
    "path": "^0.12.7",
    "validator": "^13.7.0"
  },
  "type": "module"
}
