{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle":  "off",
        "import/prefer-default-export": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "no-shadow":"off",
        "no-unused-vars": "off",
        "prefer-destructuring": "off",
        "radix": "off"
    }
}
